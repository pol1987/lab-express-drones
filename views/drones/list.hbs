<h2>Available drones</h2>

{{#each listDrone}}
    <li>
        
        <a href="/drones/{{this._id}}/edit">{{this.name}}</a>

        {{this.propellers}}

        {{this.maxSpeed}}   

        <form action="/drones/{{this._id}}/delete" method="POST">
             <button>Borrar</button>
        </form>
            <br>
    </li>  
{{/each}}

